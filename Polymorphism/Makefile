.PHONY: clean All

All:
	@echo "----------Building project:[ First-Interface - Debug ]----------"
	@cd "First-Interface" && "$(MAKE)" -f  "First-Interface.mk"
clean:
	@echo "----------Cleaning project:[ First-Interface - Debug ]----------"
	@cd "First-Interface" && "$(MAKE)" -f  "First-Interface.mk" clean
